{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Cli Mock Query",
      "program": "${workspaceFolder}/src/cli/cliEntry.js",
      "cwd": "${workspaceFolder}",
      "internalConsoleOptions": "openOnSessionStart",
      "args": ["--infra", "src/providers/mock/test/MockStack.js", "plan"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Cli Mock List",
      "program": "${workspaceFolder}/src/cli/cliEntry.js",
      "cwd": "${workspaceFolder}",
      "internalConsoleOptions": "openOnSessionStart",
      "args": [
        "--infra",
        "src/providers/mock/test/MockStack.js",
        "list",
        "--all"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Cli Mock Deploy",
      "program": "${workspaceFolder}/src/cli/cliEntry.js",
      "cwd": "${workspaceFolder}",
      "internalConsoleOptions": "openOnSessionStart",
      "args": ["--infra", "src/providers/mock/test/MockStack.js", "deploy"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Cli Mock Destroy",
      "program": "${workspaceFolder}/src/cli/cliEntry.js",
      "cwd": "${workspaceFolder}",
      "internalConsoleOptions": "openOnSessionStart",
      "args": ["--infra", "src/providers/mock/test/MockStack.js", "destroy"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Cli Mock Destroy By Name",
      "program": "${workspaceFolder}/src/cli/cliEntry.js",
      "cwd": "${workspaceFolder}",
      "internalConsoleOptions": "openOnSessionStart",
      "args": [
        "--infra",
        "src/providers/mock/test/MockStack.js",
        "destroy",
        "--name",
        "pippo"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Cli Aws Plan",
      "program": "${workspaceFolder}/src/cli/cliEntry.js",
      "cwd": "${workspaceFolder}/examples/aws/",
      "internalConsoleOptions": "openOnSessionStart",
      "args": ["plan"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Cli Aws Status",
      "program": "${workspaceFolder}/src/cli/cliEntry.js",
      "cwd": "${workspaceFolder}/examples/aws/",
      "internalConsoleOptions": "openOnSessionStart",
      "args": ["status"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Cli Aws List",
      "program": "${workspaceFolder}/src/cli/cliEntry.js",
      "cwd": "${workspaceFolder}/examples/aws/",
      "internalConsoleOptions": "openOnSessionStart",
      "args": ["list", "--all"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Cli Aws Deploy",
      "program": "${workspaceFolder}/src/cli/cliEntry.js",
      "cwd": "${workspaceFolder}/examples/aws/",
      "internalConsoleOptions": "openOnSessionStart",
      "args": ["--infra", "examples/aws/iac.js", "deploy"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Cli Aws Destroy",
      "program": "${workspaceFolder}/src/cli/cliEntry.js",
      "cwd": "${workspaceFolder}/examples/aws/",
      "internalConsoleOptions": "openOnSessionStart",
      "args": ["destroy"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Cli Google Plan",
      "program": "${workspaceFolder}/src/cli/cliEntry.js",
      "cwd": "${workspaceFolder}/examples/google/",
      "internalConsoleOptions": "openOnSessionStart",
      "args": ["plan"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Cli Google Status",
      "program": "${workspaceFolder}/src/cli/cliEntry.js",
      "cwd": "${workspaceFolder}/examples/google/",
      "internalConsoleOptions": "openOnSessionStart",
      "args": ["status"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Cli Google List",
      "program": "${workspaceFolder}/src/cli/cliEntry.js",
      "cwd": "${workspaceFolder}/examples/google/",
      "internalConsoleOptions": "openOnSessionStart",
      "args": ["--infra", "examples/google/iac.js", "list", "--all"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Cli Google Deploy",
      "program": "${workspaceFolder}/src/cli/cliEntry.js",
      "cwd": "${workspaceFolder}/examples/google/",
      "internalConsoleOptions": "openOnSessionStart",
      "args": ["--infra", "examples/google/iac.js", "deploy"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Cli Google Destroy",
      "program": "${workspaceFolder}/src/cli/cliEntry.js",
      "cwd": "${workspaceFolder}/examples/google/",
      "internalConsoleOptions": "openOnSessionStart",
      "args": ["--infra", "examples/google/iac.js", "destroy"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Cli Multi Status",
      "program": "${workspaceFolder}/src/cli/cliEntry.js",
      "cwd": "${workspaceFolder}/examples/multi/",
      "args": ["status"]
    },
    {
      "name": "Debug test",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
      "args": [
        "--no-timeouts",
        "--colors",
        "--config",
        "${workspaceRoot}/.mocharc.json",
        "'src/**/*.test.js'"
      ],
      "internalConsoleOptions": "openOnSessionStart",
      "cwd": "${workspaceRoot}/",
      "env": {
        "NODE_PATH": "src"
      }
    }
  ]
}
